<Window 
    x:Uid="Window_1" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    mc:Ignorable="d" 
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" 
    xmlns:toolkit="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
    x:Class="VideoConvert.Windows.SettingsWindow" 
    Title="Settings" 
    Loaded="WindowLoaded" 
    WindowStartupLocation="CenterOwner" 
    ResizeMode="NoResize" 
    Height="744" 
    Width="810" 
    DataContext="{Binding RelativeSource={RelativeSource Self}}" 
    FontFamily="Verdana" 
    SnapsToDevicePixels="True" 
    WindowStyle="SingleBorderWindow" Closing="WindowClosing">
    <Grid x:Uid="MainGrid" Width="790" Name="MainGrid">
        <TabControl x:Uid="SettingsTabControl" Height="667" HorizontalAlignment="Left" x:Name="SettingsTabControl" VerticalAlignment="Top" Width="790">
            <TabItem x:Uid="GeneralTab" Header="General" x:Name="GeneralTab">
                <Grid x:Uid="Grid_1">
                    <GroupBox x:Uid="BDInfoGroup" Header="BDInfo Library" Margin="10,10,0,0" x:Name="BDInfoGroup" VerticalAlignment="Top" HorizontalAlignment="Left" Width="764">
                        <Grid x:Uid="Grid_2">
                            <CheckBox x:Uid="KeepOrder" Content="Keep original stream ordering" Height="16" HorizontalAlignment="Left" Margin="6,6,0,0" x:Name="KeepOrder" VerticalAlignment="Top" IsChecked="{Binding Path=Default.KeepStreamOrder, UpdateSourceTrigger=Explicit, Mode=TwoWay, Source={StaticResource Settings}}" />
                            <CheckBox x:Uid="FilterLoops" Content="Filter playlists that contain loops" Height="16" HorizontalAlignment="Left" Margin="6,28,0,0" x:Name="FilterLoops" VerticalAlignment="Top" IsChecked="{Binding Path=Default.FilterLoopingPlaylists, Source={StaticResource Settings}, UpdateSourceTrigger=Explicit, Mode=TwoWay}" />
                            <CheckBox x:Uid="FilterShort" Content="Filter playlists with length" Height="16" HorizontalAlignment="Left" Margin="6,50,0,0" x:Name="FilterShort" VerticalAlignment="Top" IsChecked="{Binding Path=Default.FilterShortPlaylists, Source={StaticResource Settings}, UpdateSourceTrigger=Explicit, Mode=TwoWay}" />
                            <Label x:Uid="FilterShortLessLabel" Content="&lt;" Height="21" HorizontalAlignment="Right" Margin="0,47,496,0" Name="FilterShortLessLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                            <TextBox x:Uid="FilterShortValue" Height="23" HorizontalAlignment="Right" Margin="0,47,436,0" x:Name="FilterShortValue" VerticalAlignment="Top" Width="55" IsEnabled="{Binding ElementName=FilterShort, Path=IsChecked}" Text="{Binding Source={StaticResource Settings}, Path=Default.FilterShortPlaylistsValue}" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                            <Label x:Uid="FilterShortSecLabel" Content="sec" Height="21" Margin="0,47,121,0" Name="FilterShortSecLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalAlignment="Right" Width="307" />
                            <CheckBox x:Uid="EnableSSIF" Content="Enable SSIF scanning" Height="16" HorizontalAlignment="Left" Margin="6,72,0,0" x:Name="EnableSSIF" VerticalAlignment="Top" IsChecked="{Binding Path=Default.EnableSSIF, Source={StaticResource Settings}, UpdateSourceTrigger=Explicit, Mode=TwoWay}" />
                        </Grid>
                    </GroupBox>
                    <CheckBox x:Uid="DeleteCompletedJobs" Content="Remove completed jobs from list" Height="16" HorizontalAlignment="Left" Margin="16,197,0,0" x:Name="DeleteCompletedJobs" VerticalAlignment="Top" IsChecked="{Binding Path=Default.DeleteCompletedJobs, Source={StaticResource Settings}, UpdateSourceTrigger=Explicit, Mode=TwoWay}" />
                    <CheckBox x:Uid="DeleteTemporaryFiles" Height="16" HorizontalAlignment="Left" Margin="16,219,0,0" x:Name="DeleteTemporaryFiles" VerticalAlignment="Top" Content="Remove temporary files after finish" IsChecked="{Binding Path=Default.DeleteTemporaryFiles, Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" />
                    <Label x:Uid="ProcessPriorityLabel" Content="Process priority" Height="21" HorizontalAlignment="Left" Margin="16,133,0,0" x:Name="ProcessPriorityLabel" VerticalAlignment="Top" VerticalContentAlignment="Center" Padding="0" />
                    <ComboBox x:Uid="ProcessPriority" Height="21" HorizontalAlignment="Left" Margin="169,133,0,0" x:Name="ProcessPriority" VerticalAlignment="Top" Width="200" VerticalContentAlignment="Center" Padding="5,0" HorizontalContentAlignment="Center" SelectedIndex="{Binding Mode=TwoWay, UpdateSourceTrigger=Explicit, Path=Default.ProcessPriority, Source={StaticResource Settings}}">
                        <ComboBoxItem x:Uid="ComboBoxItem_1" Content="Realtime" />
                        <ComboBoxItem x:Uid="ComboBoxItem_2" Content="High" />
                        <ComboBoxItem x:Uid="ComboBoxItem_3" Content="Higher" />
                        <ComboBoxItem x:Uid="ComboBoxItem_4" Content="Normal" IsSelected="False" />
                        <ComboBoxItem x:Uid="ComboBoxItem_5" Content="Lower" />
                        <ComboBoxItem x:Uid="ComboBoxItem_6" Content="Low" />
                    </ComboBox>
                    
                    <Label x:Uid="LanguageLabel" Content="Language" Height="21" HorizontalAlignment="Left" Margin="16,158,0,0" x:Name="LanguageLabel" VerticalAlignment="Top" VerticalContentAlignment="Center" Padding="0" />
                    <ComboBox x:Uid="LanguageSelect" Height="21" HorizontalAlignment="Left" Margin="169,158,0,0" x:Name="LanguageSelect" VerticalAlignment="Top" Width="200" VerticalContentAlignment="Center" Padding="5,0" HorizontalContentAlignment="Center" SelectedValue="{Binding Path=Default.UseLanguage, Mode=TwoWay, UpdateSourceTrigger=Explicit, Source={StaticResource Settings}}" SelectedValuePath="LangCode" DisplayMemberPath="LangName" ItemsSource="{Binding Path=Langs}"/>
                    <CheckBox x:Uid="UseHardwareRendering" Content="Use Hardware rendering" Height="16" HorizontalAlignment="Left" IsChecked="{Binding Path=Default.UseHardwareRendering, Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Margin="16,283,0,0" Name="UseHardwareRendering" VerticalAlignment="Top" />
                    <CheckBox x:Uid="Use64BitEncoders" Content="Use 64 bit encoders" Height="16" HorizontalAlignment="Left" IsChecked="{Binding Path=Default.Use64bitEncoders, Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Margin="16,241,0,0" Name="Use64BitEncoders" VerticalAlignment="Top" />
                    <CheckBox x:Uid="CreateXbmcInfoFile" Content="Create XBMC info file" Height="16" HorizontalAlignment="Left" IsChecked="{Binding Path=Default.CreateXbmcInfoFile, Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Margin="16,261,0,0" Name="CreateXbmcInfoFile" VerticalAlignment="Top" />
                    <GroupBox x:Uid="AviSynthOptions" Header="AviSynth Options" Height="145" HorizontalAlignment="Left" Margin="16,0,0,23" Name="AviSynthOptions" VerticalAlignment="Bottom" Width="762">
                        <Grid x:Uid="Grid_43">
                            <CheckBox x:Uid="UseAviSynthMT" Content="Generate multi-threaded (MT) AviSynth scripts" Height="16" HorizontalAlignment="Left" IsChecked="{Binding Path=Default.UseAviSynthMT, Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Margin="7,6,0,0" Name="UseAviSynthMT" VerticalAlignment="Top" />
                            <TextBlock x:Uid="WarningAviSynthMT" Text="Warning! Activate only if you have AviSynth-MT installed!" Height="32" HorizontalAlignment="Left" Margin="27,28,0,0" Name="WarningAviSynthMT" Padding="0" VerticalAlignment="Top" Foreground="Red" FontWeight="Bold" MaxWidth="700" TextWrapping="WrapWithOverflow" />
                            <CheckBox x:Uid="UseHQDeinterlace" Content="Use high-quality deinterlacing" Height="16" HorizontalAlignment="Left" IsChecked="{Binding Path=Default.UseHQDeinterlace, Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Margin="7,64,0,0" Name="UseHQDeinterlace" VerticalAlignment="Top" />
                            <TextBlock x:Uid="UseHQDeinterlaceWarning" FontWeight="Bold" Foreground="Red" Height="32" HorizontalAlignment="Left" Margin="27,86,0,0" MaxWidth="700" Name="UseHQDeinterlaceWarning" Padding="0" Text="Warning! This deinterlacer is very slow, using AviSynth-MT is recommended!" TextWrapping="WrapWithOverflow" VerticalAlignment="Top" />
                        </Grid>
                    </GroupBox>
                    <Label x:Uid="AutoUpdateHeader" Content="Autoupdate frequency:" Height="21" HorizontalAlignment="Left" Margin="16,320,0,0" Name="AutoUpdateHeader" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                    <ComboBox x:Uid="AutoUpdateFrequency" Height="21" HorizontalAlignment="Left" Margin="169,320,0,0" Name="AutoUpdateFrequency" VerticalAlignment="Top" Width="200" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" SelectedIndex="{Binding Path=Default.UpdateFrequency ,Source={StaticResource Settings}, Mode=TwoWay, UpdateSourceTrigger=Explicit}">
                        <ComboBoxItem x:Uid="ComboBoxItem_217" Content="At start" />
                        <ComboBoxItem x:Uid="ComboBoxItem_218" Content="Daily" />
                        <ComboBoxItem x:Uid="ComboBoxItem_219" Content="Weekly" />
                    </ComboBox>
                    <Label x:Uid="LastUpdatedLabel" Height="21" HorizontalAlignment="Left" Margin="16,356,0,0" Name="LastUpdatedLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" Content="{Binding Path=Default.LastUpdateRun ,Source={StaticResource Settings}, Mode=OneWay}" ContentStringFormat="Last update: {0:dd/MM/yyyy HH:mm:ss}" />
                    <CheckBox x:Uid="ShowChangeLog" Content="Show Changelog after update" Height="16" HorizontalAlignment="Left" IsChecked="{Binding Path=Default.ShowChangeLog, UpdateSourceTrigger=Explicit, Mode=TwoWay, Source={StaticResource Settings}}" Margin="16,383,0,0" Name="ShowChangeLogAfterUpdate" VerticalAlignment="Top" />
                </Grid>
            </TabItem>
            <TabItem x:Uid="EncoderTab" Header="Encoder" x:Name="EncoderTab">
                <Grid x:Uid="Grid_3">
                    <GroupBox x:Uid="EncoderPathGroup" Header="Encoder directory" HorizontalAlignment="Left" Margin="7,6,0,0" Name="EncoderPathGroup" VerticalAlignment="Top" Width="764">
                        <Grid x:Uid="Grid_4">
                            <TextBox x:Uid="EncoderPath" Height="23" HorizontalAlignment="Left" Margin="6,6,0,0" Name="EncoderPath" Text="{Binding Source={StaticResource Settings}, Path=Default.ToolsPath}" VerticalAlignment="Top" Width="463" />
                            <Button x:Uid="SelectEncoderPath" Click="SelectEncoderPathClick" Content="Browse..." Height="23" HorizontalAlignment="Left" Margin="482,6,0,0" Name="SelectEncoderPath" VerticalAlignment="Top" Width="130" />
                            <Button x:Uid="ReloadEncoders" Click="ReloadEncodersClick" Content="Reload" Height="23" HorizontalAlignment="Left" Margin="616,6,0,0" Name="ReloadEncoders" VerticalAlignment="Top" Width="130" />
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Uid="JavaPathGroup" Header="Java directory" HorizontalAlignment="Left" Margin="7,69,0,0" Name="JavaPathGroup" VerticalAlignment="Top" Width="764">
                        <Grid x:Uid="Grid_5">
                            <TextBox x:Uid="JavaPath" Height="23" HorizontalAlignment="Left" Margin="6,6,0,0" Name="JavaPath" Text="{Binding Source={StaticResource Settings}, Path=Default.JavaInstallPath}" VerticalAlignment="Top" Width="604" />
                            <Button x:Uid="JavaPathSelection" Content="Browse..." Height="23" HorizontalAlignment="Left" Margin="616,6,0,0" Name="JavaPathSelection" VerticalAlignment="Top" Width="130" Click="JavaPathSelectionClick" />
                        </Grid>
                    </GroupBox>
                    <Expander x:Uid="AppVersionsExpander" Header="Installed encoder" x:Name="AppVersionsExpander" Margin="7,154,9,58" IsExpanded="True">
                        <ScrollViewer x:Uid="AppVersionsViewer" x:Name="AppVersionsViewer" VerticalScrollBarVisibility="Auto">
                            <Grid x:Uid="AppVersionsStack" x:Name="AppVersionsStack">
                                <Label x:Uid="x264Ver" Content="{Binding Source={StaticResource Settings}, Path=Default.Lastx264Ver}" Height="21" HorizontalAlignment="Left" Margin="10,10,0,0" x:Name="X264Ver" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="x264 Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.Lastx264Ver.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="x26464Ver" Content="{Binding Source={StaticResource Settings}, Path=Default.Lastx264_64Ver}" Height="21" HorizontalAlignment="Left" Margin="10,31,0,0" x:Name="X26464Ver" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="x264 Version {0:s} (64 bit)" Visibility="{Binding Source={StaticResource Settings}, Path=Default.Lastx264_64Ver.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="ffmpegVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastffmpegVer}" Height="21" HorizontalAlignment="Left" Margin="10,52,0,0" x:Name="FfmpegVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="ffmpeg Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastffmpegVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="ffmpeg64Ver" Content="{Binding Source={StaticResource Settings}, Path=Default.Lastffmpeg_64Ver}" Height="21" HorizontalAlignment="Left" Margin="10,73,0,0" x:Name="Ffmpeg64Ver" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="ffmpeg Version {0:s} (64 bit)" Visibility="{Binding Source={StaticResource Settings}, Path=Default.Lastffmpeg_64Ver.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="eac3toVer" Content="{Binding Source={StaticResource Settings}, Path=Default.Lasteac3toVer}" Height="21" HorizontalAlignment="Left" Margin="10,94,0,0" x:Name="Eac3ToVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" Visibility="{Binding Source={StaticResource Settings}, Path=Default.Lasteac3toVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" ContentStringFormat="eac3to Version {0:s}" />
                                <Label x:Uid="lsdvdVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastlsdvdVer}" Height="21" HorizontalAlignment="Left" Margin="10,115,0,0" x:Name="LsdvdVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="lsdvd Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastlsdvdVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="mkvmergeVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastMKVMergeVer}" Height="21" HorizontalAlignment="Left" Margin="10,136,0,0" x:Name="MkvmergeVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastMKVMergeVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" ContentStringFormat="mkvmerge Version {0:s}" />
                                <Label x:Uid="mplayerVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastMplayerVer}" Height="21" HorizontalAlignment="Left" Margin="10,157,0,0" x:Name="MplayerVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="MPlayer Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastMplayerVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="TSMuxerVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastTSMuxerVer}" Height="21" HorizontalAlignment="Left" Margin="10,178,0,0" x:Name="TSMuxerVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastTSMuxerVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" ContentStringFormat="tsMuxeR Version {0:s}" />
                                <Label x:Uid="MJPEGtoolsVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastMJPEGtoolsVer}" Height="21" HorizontalAlignment="Left" Margin="10,199,0,0" x:Name="MJPEGToolsVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="mjpegtools Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastMJPEGtoolsVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="DVDAuthorVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastDVDAuthorVer}" Height="21" HorizontalAlignment="Left" Margin="10,220,0,0" x:Name="DVDAuthorVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="dvdauthor Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastDVDAuthorVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="MP4BoxVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastMp4BoxVer}" Height="21" HorizontalAlignment="Left" Margin="10,241,0,0" x:Name="MP4BoxVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="MP4Box Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastMp4BoxVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="HcEncVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastHcEncVer}" Height="21" HorizontalAlignment="Left" Margin="10,262,0,0" x:Name="HcEncVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="HCenc Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastHcEncVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="OggEncVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastOggEncVer}" Height="21" HorizontalAlignment="Left" Margin="10,283,0,0" x:Name="OggEncVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="OggEnc Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastOggEncVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="NeroAACEncVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastNeroAacEncVer}" Height="21" HorizontalAlignment="Left" Margin="10,304,0,0" x:Name="NeroAACEncVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="NeroAACEnc Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastNeroAacEncVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="LameVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastLameVer}" Height="21" HorizontalAlignment="Left" Margin="10,325,0,0" x:Name="LameVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="Lame Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastLameVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="VpxEncVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastVpxEncVer}" Height="21" HorizontalAlignment="Left" Margin="10,346,0,0" x:Name="VpxEncVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="VpxEnc Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastVpxEncVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="BDSup2SubVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastBDSup2SubVer}" Height="21" HorizontalAlignment="Left" Margin="10,367,0,0" x:Name="BDSup2SubVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="BDSup2Sub Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastBDSup2SubVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                                <Label x:Uid="BDInfoVer" Height="21" HorizontalAlignment="Left" Margin="10,388,0,0" x:Name="BDInfoVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="BDInfo Version {0:s} (compiled as Library)" />
                                <Label x:Uid="MediaInfoVer" Height="21" HorizontalAlignment="Left" Margin="10,409,0,0" x:Name="MediaInfoVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                                <Label x:Uid="AviSynthVer" Content="{Binding Source={StaticResource Settings}, Path=Default.LastAviSynthVer}" Height="21" HorizontalAlignment="Left" Margin="10,430,0,0" x:Name="AviSynthVer" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ContentStringFormat="AviSynth Version {0:s}" Visibility="{Binding Source={StaticResource Settings}, Path=Default.LastAviSynthVer.Length, Converter={StaticResource StringLengthVisiblityConverter}}" />
                            </Grid>
                        </ScrollViewer>
                    </Expander>
                    <Label x:Uid="NeroAacEncGetDescription" Content="For AAC-Audio support, download NeroAacEnc from the link below and extract to the encoder directory" Height="21" HorizontalAlignment="Left" Margin="7,584,0,0" Name="NeroAacEncGetDescription" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                    <Label x:Uid="NeroAacEncGetLink" Content="http://www.nero.com/eng/downloads-nerodigital-nero-aac-codec.php" Height="21" HorizontalAlignment="Left" Margin="7,604,0,0" Name="NeroAacEncGetLink" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" ForceCursor="True" Cursor="Hand" FontWeight="Bold" FontStyle="Italic" MouseLeftButtonUp="NeroAacEncGetLinkMouseLeftButtonUp" />
                </Grid>
            </TabItem>
            <TabItem x:Uid="PathSettingsTab" Header="Directories" x:Name="PathSettingsTab">
                <Grid x:Uid="Grid_7">
                    <GroupBox x:Uid="TempPathGroup" Header="Temp. files directory" HorizontalAlignment="Left" Margin="7,6,0,0" Name="TempPathGroup" VerticalAlignment="Top" Width="764">
                        <Grid x:Uid="Grid_8">
                            <TextBox x:Uid="DemuxLocation" Height="23" HorizontalAlignment="Left" Margin="6,6,0,0" Name="DemuxLocation" Text="{Binding Source={StaticResource Settings}, Path=Default.DemuxLocation, Mode=TwoWay, UpdateSourceTrigger=Explicit}" VerticalAlignment="Top" Width="604" />
                            <Button x:Uid="SelectDemuxLocation" Click="SelectDemuxLocationClick" Content="Browse..." Height="23" HorizontalAlignment="Right" Margin="0,6,6,0" Name="SelectDemuxLocation" VerticalAlignment="Top" Width="130" />
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Uid="OutputPathGroup" Header="Output directory" HorizontalAlignment="Left" Margin="7,77,0,0" Name="OutputPathGroup" VerticalAlignment="Top" Width="764">
                        <Grid x:Uid="Grid_9">
                            <TextBox x:Uid="OutputLocation" Height="23" HorizontalAlignment="Left" Margin="6,6,0,0" Name="OutputLocation" Text="{Binding Source={StaticResource Settings}, Path=Default.OutputLocation}" VerticalAlignment="Top" Width="604" />
                            <Button x:Uid="BrowseOutputLocation" Click="BrowseOutputLocationClick" Content="Browse..." Height="23" HorizontalAlignment="Right" Margin="0,6,6,0" Name="BrowseOutputLocation" VerticalAlignment="Top" Width="130" />
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem x:Uid="XbmcInfoTab" x:Name="XbmcInfoTab" Header="XBMC info settings">
                <Grid x:Uid="Grid_44">
                    <GroupBox x:Uid="MovieDBSettingsGroup" Header="TheMovieDB.com settings" Height="164" HorizontalAlignment="Left" Margin="6,6,0,0" Name="MovieDBSettingsGroup" VerticalAlignment="Top" Width="772">
                        <Grid x:Uid="Grid_45">
                            <Label x:Uid="MovieDBPreferredSearchLangLabel" Content="Preferred search Language:" Height="18" HorizontalAlignment="Left" Margin="10,10,0,0" Name="MovieDBPreferredSearchLangLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                            <ComboBox x:Uid="MovieDBPreferredSearchLanguage" DisplayMemberPath="Name" Height="23" HorizontalAlignment="Left" Margin="184,10,0,0" Name="MovieDBPreferredSearchLanguage" SelectedValuePath="Code" VerticalAlignment="Top" Width="175" HorizontalContentAlignment="Center" />
                            <Label x:Uid="MovieDBCertCountryLabel" Content="Certification Country:" Height="18" HorizontalAlignment="Left" Margin="10,39,0,0" Name="MovieDBCertCountryLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                            <ComboBox x:Uid="MovieDBCertCountry" DisplayMemberPath="CountryName" Height="23" HorizontalAlignment="Left" Margin="184,39,0,0" Name="MovieDBCertCountry" SelectedValuePath="CountryName" VerticalAlignment="Top" Width="175" SelectionChanged="MovieDBCertCountry_SelectionChanged" HorizontalContentAlignment="Center" />
                            <Label x:Uid="MovieDBFallbackSearchLangLabel" Content="Fallback:" Height="18" HorizontalAlignment="Right" Margin="0,10,288,0" Name="MovieDBFallbackSearchLangLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                            <ComboBox x:Uid="MovieDBFallbackSearchLanguage" DisplayMemberPath="Name" Height="23" HorizontalAlignment="Right" Margin="0,10,26,0" Name="MovieDBFallbackSearchLanguage" SelectedValuePath="Code" VerticalAlignment="Top" Width="175" HorizontalContentAlignment="Center" />
                            <Label x:Uid="MovieDBFallbackCertCountryLabel" Content="Fallback:" Height="18" HorizontalAlignment="Right" Margin="0,39,288,0" Name="MovieDBFallbackCertCountryLabel" Padding="0" VerticalAlignment="Top" VerticalContentAlignment="Center" />
                            <ComboBox x:Uid="MovieDBFallbackCertCountry" DisplayMemberPath="CountryName" Height="23" HorizontalAlignment="Right" Margin="0,39,26,0" Name="MovieDBFallbackCertCountry" SelectedValuePath="CountryName" VerticalAlignment="Top" Width="175" SelectionChanged="MovieDBFallbackCertCountry_SelectionChanged" HorizontalContentAlignment="Center" />
